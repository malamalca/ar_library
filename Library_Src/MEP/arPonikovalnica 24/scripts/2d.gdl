PEN penUndergroundLINE_TYPE lineUndergroundIF kind = KIND_GENERIC_ROUND THEN	arrowCount = 8	FOR i = 1 TO arrowCount		LINE2 A/2, 0, A/2 + latchSize/2, 0		LINE2 A/2 + latchSize/2, 0, A/2 + latchSize/3, latchSize/8		LINE2 A/2 + latchSize/2, 0, A/2 + latchSize/3, -latchSize/8		ROT2 360 / arrowCount	NEXT i	DEL TOPENDIF