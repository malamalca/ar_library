MAX_NODES = 20nix = 0.001ND_OFFSET = 1ND_ELEVATION = 2ND_SHAFTSIZE = 3ND_SHAFTHEIGHT = 4ND_INLETLEFT = 5ND_INLETRIGHT = 6ND_PIPEDIAMETER = 7ND_TEXT_TITLE = 1nodeCount = 0for i = 1 to n	if (i = 1) or (abs(nodes[i][ND_OFFSET]) > nix) then		for j = 1 to 7			put nodes[i][j]		next j		nodeCount = nodeCount + 1	endifnext in = nodeCountfor i = 1 to n	for j = 1 to 7		nodes[i][j] = get(1)	next jnext ifor i = n+1 to MAX_NODES	for j = 1 to 7		nodes[i] = nodes[n]	next jnext i