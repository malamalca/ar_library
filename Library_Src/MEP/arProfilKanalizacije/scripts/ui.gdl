ui_dialog "Profil Kanalizacije"ui_current_page gs_ui_current_pagexLeft = 20yTop = 5yLabel = 14yRow = 40iPageIDRoot 			= -1iPageIDSettings			=  1iPageIDShafts			=  2! ==============================================================================! UI_PAGES! ==============================================================================	ui_page iPageIDSettings, iPageIDRoot, "2D Settings"		gosub "basicSettings"	ui_page iPageIDShafts, iPageIDRoot, "Shaft Settings"		gosub "shaftSettings"end! ==============================================================================! SUBROUTINES! ==============================================================================! ------------------------------------------------------------------------------"basicSettings":! ------------------------------------------------------------------------------aRow = 1xOffset = 0ui_outfield "Elevation from Sea Level:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "base_level", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 100, 25aRow = 2xOffset = 0ui_outfield "Table Text Font:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "font_td", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 190xOffset = 200ui_outfield "Font Size:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "font_td_size", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 60aRow = 3.5xOffset = 0ui_outfield "2D Representation:", xLeft + xOffset, yTop + (aRow - 1) * yRowaRow = 4xOffset = 0ui_outfield "Text:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "pen_text", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 40xOffset = 70ui_outfield "Grid:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "pen_grid", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 40xOffset = 140ui_outfield "Shaft:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "pen_shaft", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 40xOffset = 210ui_outfield "Pipe:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "pen_pipe", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 40return! ------------------------------------------------------------------------------"shaftSettings":! ------------------------------------------------------------------------------aRow = 1xOffset = 0ui_outfield "Shaft:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_selected_shaft", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 50, 25xOffset = 200ui_outfield "Title:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_title", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25aRow = 2xOffset = 0ui_outfield "Offset [m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_offset", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25xOffset = 200ui_outfield "Size (Width) [m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_size", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25aRow = 3xOffset = 0ui_outfield "Elevation [m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_elevation", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25xOffset = 200ui_outfield "Height [m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_height", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25aRow = 4xOffset = 0ui_outfield "Pipe d[m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_pipediameter", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25aRow = 5xOffset = 0ui_outfield "Left Inlet Offset:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_inletleft", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25xOffset = 200ui_outfield "Right Inlet Offset:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaft_inletright", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25aRow = 6xOffset = 0ui_outfield "Extension Width:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaftextension_width", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25xOffset = 200ui_outfield "Extension Height:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield{3} "ui_shaftextension_height", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 150, 25return