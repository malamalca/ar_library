unId = 1HOTSPOT2 0, 0, unId: unId = unId + 1!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Draw LATCHADD2 deltaX, deltaYHOTSPOT2 0, 0, unId, deltaX, 1: unId = unId + 1!HOTSPOT2 0, 0, unId, deltaX, 2: unId = unId + 1PEN penLatchLINE_TYPE lineLatchFILL fillTypeIF latchKind = HATCH_ROUND THEN	POLY2_B 2, 		FRAMEFILL_DRAWCONTOUR + FRAMEFILL_DRAWFILL + FRAMEFILL_LOCALFILLORIENTATION, 		penFill, penFillBg,		0, 0, 900,		latchSize/2, 360, 4001			HOTSPOT2 -latchSize/2, -latchSize/2, unId: unId = unId + 1	HOTSPOT2 -latchSize/2, latchSize/2, unId: unId = unId + 1	HOTSPOT2 latchSize/2, -latchSize/2, unId: unId = unId + 1	HOTSPOT2 latchSize/2, latchSize/2, unId: unId = unId + 1	HOTARC2 0, 0, latchSize/2, 0, 360, unId: unId = unId + 1ENDIFIF latchKind = HATCH_RECTANGLE THEN	ADD2 -latchSize/2, -latchSize/2	POLY2_B 5,		FRAMEFILL_DRAWCONTOUR + FRAMEFILL_DRAWFILL + FRAMEFILL_LOCALFILLORIENTATION, 		penFill, penFillBg,		0, 0, 1,		latchSize, 0, 1,		latchSize, latchSize, 1,		0, latchSize, 1,		0, 0, -1				HOTLINE2 0, 0, latchSize, 0	HOTLINE2 0, latchSize, latchSize, latchSize	HOTLINE2 0, 0, 0, latchSize	HOTLINE2 latchSize, 0, latchSize, latchSize	HOTSPOT2 latchSize/2, latchSize/2, unId: unId = unId + 1	HOTSPOT2 latchSize/2, 0, unId: unId = unId + 1	HOTSPOT2 latchSize/2, latchSize, unId: unId = unId + 1	HOTSPOT2 0, latchSize/2, unId: unId = unId + 1	HOTSPOT2 latchSize, latchSize/2, unId: unId = unId + 1	DEL 1ENDIFIF showAxis THEN	extensionFactor = 1.2	PEN penAxis	LINE_TYPE lineAxis	LINE2 0, latchSize/2 * extensionFactor, 0, -latchSize/2 * extensionFactor	LINE2 latchSize/2 * extensionFactor, 0, -latchSize/2 * extensionFactor, 0ENDIFDEL 1!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Draw underground shapePEN penUndergroundLINE_TYPE lineUndergroundIF kind = KIND_GENERIC_ROUND THEN	CIRCLE2 0, 0, A/2	CIRCLE2 0, 0, A/2 - wallWidth	HOTARC2 0, 0, A/2, 0, 360, unId: unId = unId + 1ENDIFIF kind = KIND_GENERIC_RECTANGLE THEN	RECT2 -A/2, -B/2, A/2, B/2ENDIF