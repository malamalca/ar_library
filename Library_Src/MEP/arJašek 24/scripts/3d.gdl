model solidpen SYMB_VIEW_PENmaterial matUndergroundunId = 1hotspot 0, 0, 0, unId, ZZYZX, 1: unId = unId + 1hotspot 0, 0, ZZYZX, unId, ZZYZX, 2: unId = unId + 1hotspot 0, 0, -1, unId, ZZYZX, 3: unId = unId + 1hotspot deltaX, deltaY, ZZYZX, unId: unId = unId + 1shaftHeight = ZZYZX - latchExtensionif abs(bottomThickness) > NIX then	shaftHeight = shaftHeight - bottomThickness	cylind bottomThickness, A/2	addz bottomThicknessendifif abs(topThickness) > NIX then	shaftHeight = shaftHeight - topThickness	addz shaftHeight	prism_ 12, topThickness,	-A/2,0, 13+66,	0,0,900,	0, 180, 4000+13+66,	0,0,900,	0, 180, 4000+13+66,	-A/2, 0, -1,	deltaX - latchSize/2,	0,		13,	deltaX,			deltaY,	900, ! set centerpoint	0, 				-180,	4013,	deltaX,			deltaY,	900,	0,				-180,	4013,	deltaX - latchSize/2,	0,		-1	if abs(latchExtension) > NIX then		addz topThickness		hotspot deltaX, deltaY, 0, unId, latchExtension, 1: unId = unId + 1		hotspot deltaX, deltaY, latchExtension, unId, latchExtension, 2: unId = unId + 1		hotspot deltaX, deltaY, -1, unId, latchExtension, 3: unId = unId + 1		add deltaX, deltaY, 0		prism_ 12, latchExtension,			-latchSize/2 - wallWidth,0, 13+66,			0,0,900,			0, 180, 4000+13+66,			0,0,900,			0, 180, 4000+13+66,			-latchSize/2 - wallWidth, 0, -1,			-latchSize/2, 0, 13,			0,0,900,			0, -180, 4013,			0,0,900,			0,-180, 4013,			-latchSize/2 - wallWidth, 0, -1		del 2	endif	del 1endifprism_ 12, shaftHeight,	-A/2,0, 13+66,	0,0,900,	0, 180, 4000+13+66,	0,0,900,	0, 180, 4000+13+66,	-A/2, 0, -1,	A/2-wallWidth, 0, 13,	0,0,900,	0, -180, 4013,	0,0,900,	0,-180, 4013,	A/2-wallWidth,0,-1