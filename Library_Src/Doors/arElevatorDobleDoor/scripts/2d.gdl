dep = 0.08unId = 1line_property 2pen gs_Pen_2D! opening part (front of door)wallhole2 4, 1+2+4, penFill, penFill,		0,		ac_wido_sill,0,		-A/2,	ac_wido_sill,		1,		-A/2,	-WALL_THICKNESS+dep,	1,		A/2,	-WALL_THICKNESS+dep,	1,		A/2,	ac_wido_sill,		1wallline2  A/2, 0, A/2, -WALL_THICKNESS+depwallline2 -A/2, 0, -A/2, -WALL_THICKNESS+dephotspot2 -A/2 + frame_width, -WALL_THICKNESS+dep, unId, open_width, 1: unId = unId + 1hotspot2 -A/2 + frame_width + open_width, -WALL_THICKNESS+dep, unId, open_width, 2: unId = unId + 1hotspot2 -A/2 - 1, -WALL_THICKNESS+dep, unId, open_width, 3: unId = unId + 1hotspot2 -A/2, 0, unId: unId = unId + 1hotspot2 A/2, 0, unId: unId = unId + 1! door niche partwallhole2 4, 1+2+4, penFill, penFill,		0,0,0,		-A/2 - side_offset,	-WALL_THICKNESS+dep,		1,		-A/2 - side_offset,	-WALL_THICKNESS,	1,		A + side_offset - frame_width * 2 - pocket_offset,	-WALL_THICKNESS,	1,		A + side_offset - frame_width * 2 - pocket_offset,	-WALL_THICKNESS+dep,		1! middle linewallline2 -A/2 - side_offset + frame_width, -WALL_THICKNESS+dep, A + side_offset - frame_width * 2 - pocket_offset, -WALL_THICKNESS+dep! back linewallline2 -A/2 - side_offset + frame_width, -WALL_THICKNESS+dep-ac_wido_sill, A + side_offset - frame_width * 2 - pocket_offset, -WALL_THICKNESS+dep-ac_wido_sill! side lineswallline2 -A/2 - side_offset, -WALL_THICKNESS+dep, -A/2 - side_offset, -WALL_THICKNESSwallline2 A + side_offset - frame_width * 2 - pocket_offset, -WALL_THICKNESS+dep, A + side_offset - frame_width * 2 - pocket_offset, -WALL_THICKNESShotspot2 A + side_offset - frame_width * 2 - pocket_offset, -WALL_THICKNESS, unId: unId = unId + 1hotspot2 -A/2 - side_offset, -WALL_THICKNESS, unId: unId = unId + 1! door framepoly2_b 5, 1+2+64, penFill, penFillBg,	-A/2,				ac_wido_sill,		1,	-A/2 + frame_width,	ac_wido_sill,			1,	-A/2 + frame_width,	-WALL_THICKNESS,			1,	-A/2,				-WALL_THICKNESS,		1,	-A/2,				ac_wido_sill,		-1poly2_b 5, 1+2+64, penFill, penFillBg,	A/2,				ac_wido_sill,		1,	A/2 - frame_width,	ac_wido_sill,			1,	A/2 - frame_width,	-WALL_THICKNESS+dep,			1,	A/2,				-WALL_THICKNESS+dep,		1,	A/2,				ac_wido_sill,		-1! sliding panelsline_property 0pen gs_Pen_2Dfill panelFillspacing = 0.005panelThickness = (dep - spacing * 3) / 2panelOffset = side_offset / 2  - frame_width / 2add2 open_width - A/2 + frame_width / 2, -WALL_THICKNESS+dep - panelThickness*2 - spacing*2poly2_b 5, 1+2+64, penFill, penFillBg,	0 - panelOffset,	0,		1,	A/2 + panelOffset,	0,		1,	A/2 + panelOffset,	panelThickness,			1,	0 - panelOffset, 	panelThickness,	1,	0 - panelOffset,    0,		-1del 1add2 0 + open_width/2 - frame_width / 2 - pocket_offset / 2, -WALL_THICKNESS+dep - panelThickness - spacingpoly2_b 5, 1+2+64, penFill, penFillBg,	0 - panelOffset,    0,		1,	A/2 + panelOffset,     0,		1,	A/2 + panelOffset,     panelThickness,			1,	0 - panelOffset, panelThickness,	1,	0 - panelOffset,    0,		-1