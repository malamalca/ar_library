openingWidth = AnicheDepth = WALL_THICKNESS - WIDO_SILL!nicheDepth = 0.2set material mat_revealwallniche 5, 1, 2,	0, 0, 1, 	nicheDepth,									! depth of niche	-openingWidth/2, 	0, 				1+2+4+8+16,	openingWidth/2, 	0, 				1+2+4+8+16,	openingWidth/2, 	B, 	1+2+4+8+16,	-openingWidth/2, 	B, 	1+2+4+8+16,	-openingWidth/2, 	0, 				-1set building_material mat_insulationif rightCorner then	rotx 90	cutplane openingWidth/2, openingWidth/2 * tan(rightCornerAngle)	del 1endifif leftCorner then	rotx 90	cutplane -openingWidth/2, openingWidth/2 * tan(leftCornerAngle)	del 1endifif GLOB_STRUCTURE_DISPLAY = 0 OR GLOB_STRUCTURE_DISPLAY = 2 then	! left	GROUP "insuLeft"	if sides[_LEFT] then		prism_ 5, nicheDepth - thk_plaster,			-openingWidth/2, 	0, 		15,			-openingWidth/2 + thk_insulation,		0,		15,			-openingWidth/2 + thk_insulation,		B,		15,			-openingWidth/2, 	B,		15,			-openingWidth/2, 	0,		-1	endif	ENDGROUP	GROUP "insuRight"	if sides[_RIGHT] then		prism_ 5, nicheDepth - thk_plaster,			openingWidth/2, 	0, 		15,			openingWidth/2 - thk_insulation,		0,		15,			openingWidth/2 - thk_insulation,		B,		15,			openingWidth/2, 	B,		15,			openingWidth/2, 	0,		-1	endif	ENDGROUP	GROUP "insuTop"	if sides[_TOP] then		prism_ 5, nicheDepth - thk_plaster,			-openingWidth/2, 	B, 		15,			openingWidth/2,		B,		15,			openingWidth/2,		B - thk_insulation,		15,			-openingWidth/2, 	B - thk_insulation,		15,			-openingWidth/2, 	B,		-1	endif	ENDGROUP	GROUP "insuBottom"	if sides[_BOTTOM] then		prism_ 5, nicheDepth - thk_plaster,			-openingWidth/2, 	0, 		15,			openingWidth/2,		0,		15,			openingWidth/2,		0 + thk_insulation,		15,			-openingWidth/2, 	0 + thk_insulation,		15,			-openingWidth/2, 	0,		-1	endif	ENDGROUP		result = ADDGROUP("insuLeft", "insuRight")	result = ADDGROUP(result , "insuTop")	result = ADDGROUP(result , "insuBottom")	PLACEGROUP result endifSECT_ATTRS{2} pen_plasterset building_material mat_plaster! reveal plasterdim v[4]v = sidesif GLOB_STRUCTURE_DISPLAY = 0 then	!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!	! plaster on top of insulation	addz nicheDepth - thk_plaster		GROUP "plasterLeft"	if v[_LEFT] then		thk_plaster_reveal = 0		if plaster_sides[_LEFT] then			thk_plaster_reveal = thk_plaster		endif			! on top of insulation		prism_ 5, thk_plaster,			-openingWidth/2, 	0, 		7,			-openingWidth/2 + thk_insulation + thk_plaster_reveal,		0,		15,			-openingWidth/2 + thk_insulation  + thk_plaster_reveal,		B,		15,			-openingWidth/2, 	B,		15,			-openingWidth/2, 	0,		-1		! reveal side of insulation		if plaster_sides[_LEFT] then			addz -nicheDepth + offset_plaster + thk_plaster			prism_ 5, nicheDepth - offset_plaster - thk_plaster,				-openingWidth/2 + thk_insulation, 					0 + thk_insulation* sides[_BOTTOM], 		15,				-openingWidth/2 + thk_insulation + thk_plaster,		0 + thk_insulation* sides[_BOTTOM],		15,				-openingWidth/2 + thk_insulation + thk_plaster,		B - thk_insulation* sides[_TOP],		15,				-openingWidth/2 + thk_insulation, 					B - thk_insulation* sides[_TOP],		15,				-openingWidth/2 + thk_insulation, 					0 + thk_insulation* sides[_BOTTOM],		-1			del 1		endif	endif	ENDGROUP	GROUP "plasterRight"	if v[_RIGHT] then		thk_plaster_reveal = 0		if plaster_sides[_RIGHT] then			thk_plaster_reveal = thk_plaster		endif			prism_ 5, thk_plaster,			openingWidth/2, 	0, 		7,			openingWidth/2 - thk_insulation - thk_plaster_reveal,		0,		15,			openingWidth/2 - thk_insulation - thk_plaster_reveal,		B,		15,			openingWidth/2, 	B,		15,			openingWidth/2, 	0,		-1			! reveal side of insulation		if plaster_sides[_RIGHT] then			addz -nicheDepth + offset_plaster + thk_plaster			prism_ 5, nicheDepth - offset_plaster - thk_plaster,				openingWidth/2 - thk_insulation, 					0 + thk_insulation* sides[_BOTTOM], 		15,				openingWidth/2 - thk_insulation - thk_plaster,		0 + thk_insulation* sides[_BOTTOM],		15,				openingWidth/2 - thk_insulation - thk_plaster,		B - thk_insulation* sides[_TOP],		15,				openingWidth/2 - thk_insulation, 					B - thk_insulation* sides[_TOP],		15,				openingWidth/2 - thk_insulation, 					0 + thk_insulation* sides[_BOTTOM],		-1			del 1		endif	endif	ENDGROUP	GROUP "plasterTop"	if v[_TOP] then		thk_plaster_reveal = 0		if plaster_sides[_TOP] then			thk_plaster_reveal = thk_plaster		endif			delta_left = 0		if v[_LEFT] then			delta_left = thk_insulation		endif			prism_ 5, thk_plaster,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	B, 		7,			openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],	B,		15,			openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],	B - thk_insulation - thk_plaster_reveal,		15,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	B - thk_insulation - thk_plaster_reveal,		15,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	B,		-1			! reveal side of insulation		if plaster_sides[_TOP] then			addz -nicheDepth + offset_plaster + thk_plaster			prism_ 5, nicheDepth - offset_plaster - thk_plaster,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		B - thk_insulation - thk_plaster, 		15,				openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		B - thk_insulation - thk_plaster,		15,				openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		B - thk_insulation,		15,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		B - thk_insulation,		15,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		B - thk_insulation - thk_plaster,		-1			del 1		endif	endif	ENDGROUP	GROUP "plasterBottom"	if v[_BOTTOM] then		thk_plaster_reveal = 0		if plaster_sides[_BOTTOM] then			thk_plaster_reveal = thk_plaster		endif			prism_ 5, thk_plaster,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	0, 		15,			openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		0,		15,			openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		0 + thk_insulation + thk_plaster_reveal,		15,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	0 + thk_insulation + thk_plaster_reveal,		15,			-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 	0,		-1			! reveal side of insulation		if plaster_sides[_BOTTOM] then			addz -nicheDepth + offset_plaster + thk_plaster			prism_ 5, nicheDepth - offset_plaster - thk_plaster,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		thk_insulation, 		15,				openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		thk_insulation,		15,				openingWidth/2 - thk_insulation * sides[_RIGHT] - thk_plaster * plaster_sides[_RIGHT],		thk_insulation + thk_plaster,		15,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		thk_insulation + thk_plaster,		15,				-openingWidth/2 + thk_insulation * sides[_LEFT] + thk_plaster * plaster_sides[_LEFT], 		thk_insulation,		-1			del 1		endif		endif	ENDGROUP		plasterResult = ADDGROUP("plasterLeft", "plasterRight")	plasterResult = ADDGROUP(plasterResult, "plasterTop")	plasterResult = ADDGROUP(plasterResult, "plasterBottom")	PLACEGROUP plasterResult 		del topendifif leftCorner then	cutendendifif rightCorner then	cutendendifend