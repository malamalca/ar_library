openingWidth = AnicheDepth = WALL_THICKNESS - WIDO_SILL!wallhole 4, 1,!	-openingWidth/2, 0, 15,!	openingWidth/2, 0, 15,!	openingWidth/2, B, 15,!	-openingWidth/2, B, 15wallniche 5, 1, 2,	0, 0, 1, 	nicheDepth,									! depth of niche	-openingWidth/2, 	0, 				1+2+4+8+16,	openingWidth/2, 	0, 				1+2+4+8+16,	openingWidth/2, 	B, 	1+2+4+8+16,	-openingWidth/2, 	B, 	1+2+4+8+16,	-openingWidth/2, 	0, 				-1set building_material mat_insulation! insulationprism_ 10, nicheDepth - thk_plaster,	-openingWidth/2, 				0, 		15,	openingWidth/2, 				0,		15,	openingWidth/2, 				B,		15,	-openingWidth/2, 				B,		15,	-openingWidth/2, 				0,		-1,		-openingWidth/2 + thk_insulation, 		thk_insulation, 		15,	openingWidth/2 - thk_insulation, 		thk_insulation,			15,	openingWidth/2 - thk_insulation,		B - thk_insulation,		15,	-openingWidth/2 + thk_insulation, 		B - thk_insulation,		15,	-openingWidth/2 + thk_insulation, 		thk_insulation,			-1SECT_ATTRS{2} pen_plasterset building_material mat_plaster! front plasteraddz nicheDepth - thk_plasterprism_ 10, thk_plaster,	-openingWidth/2, 				0, 		1+4,	openingWidth/2, 				0,		1+4,	openingWidth/2, 				B,		1+4,	-openingWidth/2, 				B,		1+4,	-openingWidth/2, 				0,		-1,		-openingWidth/2 + thk_insulation, 		thk_insulation, 		15,	openingWidth/2 - thk_insulation, 		thk_insulation,			15,	openingWidth/2 - thk_insulation,		B - thk_insulation,		15,	-openingWidth/2 + thk_insulation, 		B - thk_insulation,		15,	-openingWidth/2 + thk_insulation, 		thk_insulation,			-1del top! reveal plasterdim v[4]v = visibility_plasteraddz offset_plasterprism_ 10, nicheDepth - offset_plaster,	-openingWidth/2 + thk_insulation, 				thk_insulation, 		15,	openingWidth/2 - thk_insulation, 				thk_insulation,		15,	openingWidth/2 - thk_insulation, 				B-thk_insulation,		15,	-openingWidth/2 + thk_insulation, 				B-thk_insulation,		15,	-openingWidth/2 + thk_insulation, 				thk_insulation,		-1,		-openingWidth/2 + thk_insulation + thk_plaster * v[1], 		thk_insulation + thk_plaster * v[4], 		15,	openingWidth/2 - thk_insulation - thk_plaster * v[2], 		thk_insulation + thk_plaster * v[4],			15,	openingWidth/2 - thk_insulation - thk_plaster * v[2],		B - thk_insulation - thk_plaster * v[3],		15,	-openingWidth/2 + thk_insulation + thk_plaster * v[1], 		B - thk_insulation - thk_plaster * v[3],		15,	-openingWidth/2 + thk_insulation + thk_plaster * v[1], 		thk_insulation + thk_plaster * v[4],			-1