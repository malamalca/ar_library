thk = 0.002			! metal thicknessunId = 1openingWidth = A - sideOffset * 2!wallhole 4, 1,!	-openingWidth/2, 0, 15,!	openingWidth/2, 0, 15,!	openingWidth/2, B, 15,!	-openingWidth/2, B, 15wallniche 5, 1, 2,	0, 0, -1,	WIDO_SILL,	-openingWidth/2, 0, 15,	openingWidth/2, 0, 15,	openingWidth/2, B, 15,	-openingWidth/2, B, 15,	-openingWidth/2, 0, -1!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! WALL niche for blinds box!!!!!!!!!!!!!!!!!!!!!!!!!!!!!wallniche 5, 1, 2,	0, 0, -1, 	boxWidth,									! depth of niche	-openingWidth/2, 	B, 				1+2+4+8+16,	openingWidth/2, 	B, 				1+2+4+8+16,	openingWidth/2, 	B + boxHeight, 	1+2+4+8+16,	-openingWidth/2, 	B + boxHeight, 	1+2+4+8+16,	-openingWidth/2, 	B, 				-1!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! BLINDS BOX!!!!!!!!!!!!!!!!!!!!!!!!!!!!!set building_material boxMataddx -openingWidth/2addy Broty 90prism_ 8, openingWidth,	0, 				0, 				15,	0,				boxHeight,		15,	boxWidth,		boxHeight,		15,	boxWidth,		0,				15,	boxWidth-thk, 	0,				15,	boxWidth-thk, 	boxHeight-thk, 	15,	thk,			boxHeight-thk,	15,	thk,			0,				15del topverticalOffset = 0.085!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! MOTOR box!!!!!!!!!!!!!!!!!!!!!!!!!!!!!motorBoxWidth = 0.065motorBoxHeight = 0.065motorBoxTopOffset = 0.01motorBoxOffset = - motorBoxWidth/2 + verticalOffset 			! offset from revealmotorBoxTubeWidth = 0.04motorBoxTubeThickness = 0.002addx -openingWidth/2addy B + boxHeight - motorBoxHeight - motorBoxTopOffset addz -motorBoxOffset roty 90prism_ 8, openingWidth,	0, 					0, 						15,	0,					motorBoxHeight ,		15,	motorBoxWidth,		motorBoxHeight ,		15,	motorBoxWidth,		0,						15,	motorBoxWidth-thk, 	0,						15,	motorBoxWidth-thk, 	motorBoxHeight -thk, 	15,	thk,				motorBoxHeight -thk,	15,	thk,				0,						15addy motorBoxHeight/2addx motorBoxWidth/2prism_ 12, openingWidth,		-motorBoxTubeWidth/2,0, 13+66,	0,0,900,	0, 180, 4000+13+66,	0,0,900,	0, 180, 4000+13+66,	-motorBoxTubeWidth/2, 0, -1,	motorBoxTubeWidth/2-motorBoxTubeThickness, 0, 13,	0,0,900,	0, -180, 4013,	0,0,900,	0,-180, 4013,	motorBoxTubeWidth/2-motorBoxTubeThickness,0,-1del top!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! SIDE profiles!!!!!!!!!!!!!!!!!!!!!!!!!!!!!railBottomOffset = 0.03railPWidth = 0.023railPHeight = 0.027railOffset = -railPWidth/2 + verticalOffsetrailHeight = B + 0.095 - railBottomOffsetaddy railBottomOffsetrotz 90roty 90addy openingWidth/2addx railOffsetgosub "sideProfile"del 4rotz -90roty 90addy openingWidth/2addz -railHeightaddx railOffsetgosub "sideProfile"del top!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! SLATS!!!!!!!!!!!!!!!!!!!!!!!!!!!!!slatCount = round_int((B-slatBottomOffset)/slatSpacing) + 1slatCountFolded = round_int((B-loweredDistance-slatBottomOffset)/slatSpacing) + 1addy slatBottomOffsetroty 90addz -openingWidth/2 + slatSideOffsetaddx railOffset + railPWidth / 2! first slatellaaddy B - loweredDistance - slatBottomOffsetgosub "slat"del 1for i = 2 to slatCount	dotPos = B - loweredDistance  + (slatCountFolded-1) * slatFoldedSpacing	curSlatPos = (i-1) * slatSpacing - slatBottomOffset	if curSlatPos < dotPos then		! folded slat		curSlatAngle = 0		curSlatPos = B - loweredDistance - slatBottomOffset + (i-1) * slatFoldedSpacing	else		curSlatAngle = slatAngle	endif	addy curSlatPos	rotz curSlatAngle 	gosub "slat"	del 2next idel tophotspot 0,  B,  -verticalOffset, unId, loweredDistance, 1: unId = unId + 1hotspot 0,  B - loweredDistance,  -verticalOffset, unId, loweredDistance, 2: unId = unId + 1hotspot 0,  B+1,  -verticalOffset, unId, loweredDistance, 3: unId = unId + 1hotspot 0,  B - loweredDistance + (slatCountFolded) * slatFoldedSpacing,  -verticalOffset, unId: unId = unId + 1end!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Side profile for blind!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"sideProfile":	prism_ 8, railHeight,		0, 				0, 					15,		0,				railPHeight,		15,		railPWidth,		railPHeight,		15,		railPWidth,		0,					15,		railPWidth-thk, 0,					15,		railPWidth-thk, railPHeight-thk, 	15,		thk,			railPHeight-thk,	15,		thk,			0,					15	return!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! SUB: Slat!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"slat":	addx - slatWidth / 2	prism_ 11, openingWidth - 2 * slatSideOffset,		0, 0, 15,		slatWidth * 1/4, 0.002, 15+64,		slatWidth/2, 0.0027, 15+64,		slatWidth * 3/4, 0.002, 15+64,		slatWidth, 0, 15,		slatWidth, slatThickness, 15,		slatWidth * 3/4, slatThickness + 0.002, 15+64,		slatWidth/2, slatThickness + 0.0027, 15+64,		slatWidth * 1/4, slatThickness + 0.002, 15+64,		0, slatThickness, 15,		0, 0, -1	del 1			return		