DIM fontNames[]n = request ("FONTNAMES_LIST", "", fontNames)values "stFont" fontNames, CUSTOMbreakpoint 1=1dim keynotes_lib[1][]!if GLOB_MODPAR_NAME = GLOB_ID then	CALL "keynotes_lib" PARAMETERS ALL RETURNED_PARAMETERS keynotes_lib		longText = ""	sectionText = ""	keynoteCount = int(vardim2(keynotes_lib) / 3)	for i = 1 to keynoteCount 		baseIndex = (i-1)*3+1		keynoteId = keynotes_lib[1][baseIndex]		if keynoteId = GLOB_ID then			sectionText = keynotes_lib[1][baseIndex+1]			longText = keynotes_lib[1][baseIndex+2]		endif	next i	parameters sectionText = sectionText 	parameters longText = longText!endif