values{2} "idSource" idSource_i, idSource_text! search needleneedle = GLOB_IDif idSource = 2 then	needle = keynoteendifdim keynotes_lib[1][]	CALL "keynotes_lib" PARAMETERS ALL RETURNED_PARAMETERS keynotes_lib		longText = ""	sectionText = ""	keynoteCount = int(vardim2(keynotes_lib) / 3)	for i = 1 to keynoteCount 		baseIndex = (i-1)*3+1		keynoteId = keynotes_lib[1][baseIndex]		if keynoteId = needle then			sectionText = keynotes_lib[1][baseIndex+1]			longText = keynotes_lib[1][baseIndex+2]		endif	next i	parameters sectionText = sectionText 	parameters longText = longText