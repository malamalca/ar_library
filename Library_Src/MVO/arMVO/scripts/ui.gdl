! ------------------------------------------------------------! set basic variables! ------------------------------------------------------------_yCurr		= 5_dy		= 25		! generic line spacing		_totalWidth	= 600		! fixed value to match the exisiting panels_totalHeight	= 4* _dy	! calculate the height from line spacing_stDialogTitle	= `arLibrary Settings`! ------------------------------------------------------------! set title and height for the custom MVO panel! ------------------------------------------------------------ui_dialog _stDialogTitle, _totalWidth, _totalHeight! in UI script of the MVO objectui_style 0, 1ui_outfield `Openings Settings`, 0, _yCurr, 150, 15_yCurr = _yCurr + 20ui_style 0, 0ui_outfield `Show Caption on Blinds`, 10, _yCurr, 150, 15ui_infield "blindsShowCaptionMVO", 155, _yCurr-4, 19, 19