HOTSPOT2 0,0! y axisLINE2 -marker_size/2, -marker_size/2, -marker_size/2, marker_sizeLINE2 marker_size/2, -marker_size/2, marker_size/2, marker_sizeLINE2 -marker_size/2, marker_size, 0, marker_size * 3/2LINE2 marker_size/2, marker_size, 0, marker_size * 3/2! x axisLINE2 -marker_size/2, -marker_size/2, marker_size, -marker_size/2LINE2 -marker_size/2, marker_size/2, marker_size, marker_size/2LINE2 marker_size, marker_size/2, marker_size * 3/2, 0LINE2 marker_size, -marker_size/2, marker_size * 3/2, 0! x signLINE2 marker_size/2 + marker_size/4, -marker_size/4, marker_size, marker_size/4LINE2 marker_size/2 + marker_size/4, marker_size/4, marker_size, -marker_size/4ROT2 -SYMB_ROTANGLE!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!ADD2 delta_x, delta_ytext_factor = 1IF SYMB_MIRRORED THEN	ROT2 SYMB_ROTANGLE*2	text_factor = -1ENDIFIF show_marker THEN	LINE2 -marker_size/2, 0, marker_size/2, 0	LINE2 0, -marker_size/2, 0, marker_size/2	CIRCLE2 0, 0, marker_size/6ENDIFIF show_text THEN	DEFINE STYLE "Internal" text_font, text_size / GLOB_SCALE * 100, 5, 0	ret = REQUEST ("Linear_dimension", "", dim_format)		string = GLOB_ID		IF show_coords THEN		string = string + ": X="+STR(dim_format, x2 + GLOB_WORLD_ORIGO_OFFSET_X + base_x)+"   Y="+STR(dim_format, y2+GLOB_WORLD_ORIGO_OFFSET_Y+base_y)	ENDIF		!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!	ADD2 text_pos_x, text_pos_y	ROT2 -text_angle		SET STYLE "Internal"	string_w = STW(string) / 1000 * A_	TEXT2 0, 0, string		DEL 2		!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!	! text hotspots	HOTSPOT2 text_pos_x, 0, 3, text_pos_y, 1+128	HOTSPOT2 text_pos_x, -1, 4, text_pos_y, 3	HOTSPOT2 text_pos_x, text_pos_y, 5, text_pos_y, 2		HOTSPOT2 0, text_pos_y, 6, text_pos_x, 1+128	HOTSPOT2 -1, text_pos_y, 7, text_pos_x, 3	HOTSPOT2 text_pos_x, text_pos_y, 8, text_pos_x, 2		! text angle hotspots	HOTSPOT2 text_pos_x + string_w/2 * text_factor, text_pos_y, 9,	text_angle, 4+128	HOTSPOT2 text_pos_x + ((string_w/2) * COS(text_angle)) * text_factor, text_pos_y - ((string_w/2) * SIN(text_angle)) * text_factor, 	10, text_angle, 5	HOTSPOT2 text_pos_x, text_pos_y, 11, text_angle, 6 + 512	HOTSPOT2 text_pos_x + ((string_w/2)) * text_factor, 0, 12, text_angle, 7ENDIF! goto rootIF SYMB_MIRRORED THEN	DEL 1ENDIFDEL 1! marker hotspots for positionHOTSPOT2 delta_x, 0, 13, delta_y, 1+128HOTSPOT2 delta_x, -1, 14, delta_y, 3HOTSPOT2 delta_x, delta_y, 15, delta_y, 2HOTSPOT2 0, delta_y, 16, delta_x, 1+128HOTSPOT2 -1, delta_y, 17, delta_x, 3HOTSPOT2 delta_x, delta_y, 18, delta_x, 2! marker hotspots for easy rotationDEL 1HOTSPOT2 0, marker_size * 3/2HOTSPOT2 marker_size * 3/2, 0