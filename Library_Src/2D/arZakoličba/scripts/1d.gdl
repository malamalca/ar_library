x2 = delta_x * COS(SYMB_ROTANGLE) + delta_y * SIN(SYMB_ROTANGLE)y2 = -delta_x  * SIN(SYMB_ROTANGLE) + delta_y * COS(SYMB_ROTANGLE)coord_x = ROUND_INT((GLOB_WORLD_ORIGO_OFFSET_X + x2 + base_x)*100)/100coord_y = ROUND_INT((GLOB_WORLD_ORIGO_OFFSET_Y + y2 + base_y)*100)/100bz = base_zIF use_project_z THEN    bz = GLOB_PROJECT_ALTITUDEENDIFcoord_z = ROUND_INT((SYMB_POS_Z + bz)*100)/100PARAMETERS coord_x = coord_xPARAMETERS coord_y = coord_yPARAMETERS coord_z = coord_z