curLength = 0unId = 1pen penFrameset fill plankFilldefine style "Internal" textFont, textSize, 5, 0set style "Internal"for i = 1 to n + 1	if i = n +1 then		cutLength = A - curLength 	else		cutLength = cutLengths[i]		if curLength + cutLength > A then			cutLength = A - curLength		endif	endif	if cutLength > nix then		add2 cutPositions[i][1], cutPositions[i][2]		rect2 0, -B/2, cutLength, B/2		poly2_b 4, 1+2+4,			penFill, penFillBg,			0, -B/2, 15,			0, B/2, 15,			cutLength, B/2, 15,			cutLength, -B/2, 15				text2 cutLength/2, 0, GLOB_ID				! length editing		if i < n + 1 then			hotspot2 0, 0, unId, cutLengths[i], 1: unId = unId + 1			hotspot2 cutLengths[i], 0, unId, cutLengths[i], 2: unId = unId + 1			hotspot2 -1, 0, unId, cutLengths[i], 3: unId = unId + 1		endif					del top		! position editing hotspots		hotspot2 0, cutPositions[i][2]-B/2, unId, cutPositions[i][1], 1+128: unId = unId + 1		hotspot2 cutPositions[i][1], cutPositions[i][2]-B/2, unId, cutPositions[i][1], 2: unId = unId + 1		hotspot2 -1, cutPositions[i][2]-B/2, unId, cutPositions[i][1], 3: unId = unId + 1		hotspot2 cutPositions[i][1], 0-B/2, unId, cutPositions[i][2], 1+128: unId = unId + 1		hotspot2 cutPositions[i][1], cutPositions[i][2]-B/2, unId, cutPositions[i][2], 2: unId = unId + 1		hotspot2 cutPositions[i][1], -1-B/2, unId, cutPositions[i][2], 3: unId = unId + 1		! position editing hotspots		hotspot2 0, cutPositions[i][2]+B/2, unId, cutPositions[i][1], 1+128: unId = unId + 1		hotspot2 cutPositions[i][1], cutPositions[i][2]+B/2, unId, cutPositions[i][1], 2: unId = unId + 1		hotspot2 -1, cutPositions[i][2]+B/2, unId, cutPositions[i][1], 3: unId = unId + 1		hotspot2 cutPositions[i][1], 0+B/2, unId, cutPositions[i][2], 1+128: unId = unId + 1		hotspot2 cutPositions[i][1], cutPositions[i][2]+B/2, unId, cutPositions[i][2], 2: unId = unId + 1		hotspot2 cutPositions[i][1], -1+B/2, unId, cutPositions[i][2], 3: unId = unId + 1		curLength = curLength + cutLengths[i]	endifnext i