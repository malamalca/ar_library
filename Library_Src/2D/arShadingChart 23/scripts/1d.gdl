MAX_POINTS = 20nix = 0.00001if (glob_modpar_name = "insx") or (glob_modpar_name = "insy") then	n = n + 1	x[n] = originOffsetX+insx	y[n] = originOffsetY+insy	z[n] = 0endifif not(GLOB_CONTEXT = 22) then	insx = 1	insy = 0endif!----------------------------------------! CleanUP!----------------------------------------n0 = 0nLen0 = 0nAngle0 = 0for i = 1 to n	nLen = sqr(x[i]^2 + y[i]^2)	if nLen > nix then		nAngle = acs(x[i]/nLen): if y[i] < nix then: nAngle = 360 - nAngle: endif			if nLen > nix and abs(nLen0-nLen) > nix and abs(nAngle-nAngle0) > nix then			n0 = n0 + 1			put x[i], y[i], z[i]		endif			nLen0 = nLen		nAngle0 = nAngle	endifnext in = n0! fill up points with new valuesfor i = 1 to n	x[i] = get(1)	y[i] = get(1)	z[i] = get(1)next i! clean empty points to the endfor i = n+1 to MAX_POINTS	x[i] = 0	y[i] = 0	z[i] = 0next i