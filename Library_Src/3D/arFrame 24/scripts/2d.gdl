FILL frameFill! this is Debug line!TEXT2 0,0, n!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Draw main frameIF (bottomElementHeight > 0) OR (topElementHeight > 0) THEN	RECT2 0,0, A, B	HOTLINE2 0,0, A, 0	HOTLINE2 0, B, A, BENDIFHOTSPOT2 0, B/2, unID : unID = unID+1! Editable hotspotHOTSPOT2 A, B/2, unID : unID = unID+1!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Draw In-Between StudsFOR i = 1 TO n	PEN defaultStudPen	paramDrawStudWidth = defaultStudWidth	paramDrawStudOffset = X[i]	if showStudsIn2d then		gosub "drawStud"	endif	HOTSPOT2 0, B/2, unID, X[i], 1+128   : unID = unID+1	HOTSPOT2 X[i], B/2, unID, X[i], 2    : unID = unID+1	HOTSPOT2 -1, B/2, unID, X[i], 3      : unID = unID+1NEXT i! This is "add stud" hotspotIF (n < 50) THEN	HOTSPOT2 0, B/2, unID, X[n+1], 1+128 : unID = unID+1	HOTSPOT2 -.1, B/2, unID, X[n+1], 2   : unID = unID+1	HOTSPOT2 -1, B/2, unID, X[n+1], 3    : unID = unID+1ENDIF! Helper LinesIF lblHelperLines THEN	ADD2 0,B/2	LINE_TYPE helperLineType	PEN helperLinePen	! first and last helper line	LINE2 defaultStudWidth/2, -B, defaultStudWidth/2, B 	HOTLINE2 defaultStudWidth/2, -B, defaultStudWidth/2, B 	LINE2 A-defaultStudWidth/2, -B, A-defaultStudWidth/2, B 	HOTLINE2 A-defaultStudWidth/2, -B, A-defaultStudWidth/2, B 	! helper line offset hotspots	HOTSPOT2 0, B/2, unID, helperLineOffset, 1+128   : unID = unID+1	HOTSPOT2 helperLineOffset, B/2, unID, helperLineOffset, 2    : unID = unID+1	HOTSPOT2 -1, B/2, unID, helperLineOffset, 3      : unID = unID+1	helperTmpOffset = helperLineOffset	WHILE helperTmpOffset < A-(defaultStudWidth/2) DO		LINE2 helperTmpOffset, -B, helperTmpOffset , B 		HOTLINE2 helperTmpOffset, -B, helperTmpOffset , B 		helperTmpOffset = helperTmpOffset + helperLineSpacing	ENDWHILE	DEL 1ENDIFEND!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! Draw stud subrutine! Parameters:!	paramDrawStudOffset!   paramDrawStudWidth"drawStud":	ADD2 paramDrawStudOffset - paramDrawStudWidth/2, 0	POLY2_B{2}       5,      3,      pen_fill,      pen_fill_bg, 	0,            0,            0, 	paramDrawStudWidth,            B,      1, 	paramDrawStudWidth,            0,      1, 	0,            0,      1, 	0,            B,      1, 	paramDrawStudWidth,            B,      1		LINE2 0,0, paramDrawStudWidth,B	LINE2 0,B, paramDrawStudWidth,0	HOTLINE2 0,0, paramDrawStudWidth,            B	HOTLINE2 paramDrawStudWidth,            B, paramDrawStudWidth,            0	HOTLINE2 paramDrawStudWidth,            0, 0,0	HOTLINE2 0,0, 0,B	DEL 1RETURN