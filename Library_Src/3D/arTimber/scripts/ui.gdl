ui_dialog "Timber Stud"UI_CURRENT_PAGE gs_ui_current_pagexLeft = 20yTop = 5yLabel = 14yRow = 35iPageIDRoot 			= -1iPageIDGeoLocation		=  1iPageID2DRepr			=  2iPageID3DRepr			=  3! ==============================================================================!UI_PAGES! ==============================================================================	UI_PAGE iPageIDGeoLocation, iPageIDRoot, "Dimensions", "uiTab_Dimension_hl(1)"		gosub "uiPageDimensions"	UI_PAGE iPageID2DRepr, iPageIDRoot, `2D izgled`, "uiTab_2DRepr_hl(1)"		gosub "2DRepr"	UI_PAGE iPageID3DRepr, iPageIDRoot, `3D izgled`, "uiTab_3DRepr_hl(1)"		gosub "3DRepr"END! ==============================================================================!SUBROUTINES! ==============================================================================! ------------------------------------------------------------------------------"uiPageDimensions":! ------------------------------------------------------------------------------xOffset = 0aRow = 1ui_outfield "Orientacija:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "orientation", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 200aRow = 2ui_outfield "Dolžina [m]:", xLeft + xOffset, yTop + (aRow - 1) * yRowif orientation = ORIENTATION_VERTICAL then	ui_infield "ZZYZX", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 200else	ui_infield "hor_length", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 200endifaRow = 3.5xOffset = 0for i = 1 to n	ui_button UI_FUNCTION, str(i, 1, 0), xLeft + xOffset + (i-1) * 20, yTop + (aRow - 1) * yRow, 20, 20next ireturn! ------------------------------------------------------------------------------"2DRepr":! ------------------------------------------------------------------------------xOffset = 0aRow = 1.5xOffset = 10ui_outfield "Obroba:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "contour_line_type", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 130, 30xOffset = xOffset + 150ui_outfield "Barva:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "gs_cont_pen", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 50aRow = 3xOffset = 10ui_outfield "Skrita črta:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "hidden_line_type", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 130, 30aRow = 4.5xOffset = 10ui_outfield "Šrafura:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "gs_fill_type", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 130, 30xOffset = xOffset + 150ui_outfield "Barva:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "gs_fill_pen", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 50xOffset = xOffset + 70ui_outfield "Ozadje:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "gs_back_pen", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 50return! ------------------------------------------------------------------------------"3DRepr":! ------------------------------------------------------------------------------aRow = 1.5xOffset = 10ui_outfield "Material:", xLeft + xOffset, yTop + (aRow - 1) * yRowui_infield "timber_material", xLeft + xOffset, yTop + (aRow - 1) * yRow + yLabel, 200, 30return