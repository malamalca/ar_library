!building_material timber_materialmaterial timber_materiallength = ZZYZXif orientation = ORIENTATION_HORIZONTAL then	addy -B/2	rotx 90	length = hor_lengthendiffor i = 1 to n	cutOffset = cut_lengths[i][PAR_CUT_DIST]	cutThickness = cut_lengths[i][PAR_CUT_THICKNESS1]	cutThickness2 = cut_lengths[i][PAR_CUT_THICKNESS2]	cutHeight = cut_lengths[i][PAR_CUT_HEIGHT]	rotz cut_rotations[i]	addz cutOffset	dim1 = A	dim2 = B	if cut_rotations[i] = 90 or cut_rotations[i] = 270 then		dim1 = B		dim2 = A	endif	if cut_kinds[i] = KIND_ONESIDE then		cutform 4, 1, 1,			0, 0, 1, cutHeight,			-dim1/2, -dim2/2 + cutThickness, 1+2+4+8+16,			-dim1/2, -dim2/2, 1+2+4+8+16,			dim1/2, -dim2/2, 1+2+4+8+16,			dim1/2, -dim2/2 + cutThickness, 1+2+4+8+16	endif	if cut_kinds[i] = KIND_LSHAPE then		cutform 6, 1, 1,			0, 0, 1, cutHeight,			-dim1/2, -dim2/2 + cutThickness, 1+2+4+8+16,			-dim1/2, -dim2/2, 1+2+4+8+16,			dim1/2, -dim2/2, 1+2+4+8+16,			dim1/2, dim2/2, 1+2+4+8+16,			dim1/2 - cutThickness2, dim2/2, 1+2+4+8+16,			dim1/2 - cutThickness2, -dim2/2 + cutThickness, 1+2+4+8+16	endif	del 2next iprism 4, length, 	-A/2, 	-B/2,	A/2, 	-B/2,	A/2, 	B/2,	-A/2, B/2for i = 1 to n	cutendnext i!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! HOTSPOTS!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!unId = 0hotspot 0,		0,		0, unId: unId = unId + 1hotspot -A/2,	-B/2,	0, unId: unId = unId + 1hotspot -A/2,	B/2,	0, unId: unId = unId + 1hotspot A/2,	B/2,	0, unId: unId = unId + 1hotspot A/2,	-B/2,	0, unId: unId = unId + 1hotspot 0,		0,		length, unId: unId = unId + 1hotspot 0,		0,		length, unId: unId = unId + 1hotspot -A/2,	-B/2,	length, unId: unId = unId + 1hotspot -A/2,	B/2,	length, unId: unId = unId + 1hotspot A/2,	B/2,	length, unId: unId = unId + 1hotspot A/2,	-B/2,	length, unId: unId = unId + 1if orientation = ORIENTATION_HORIZONTAL then	! horizontal length hotspot	hotspot 0, 0, 0, unId, hor_length, 1: unId = unId + 1	hotspot 0, 0, hor_length, unId, hor_length, 2, hor_length, "Horizontal Length": unId = unId + 1	hotspot 0, 0, -1, unId, hor_length, 3: unId = unId + 1endif! add cut hotspotif n < MAX_CUTS then	hotspot 0, 0, 0, unId, add_cut_distance, 1: unId = unId + 1	hotspot 0, 0, ADD_CUT_DIST, unId, add_cut_distance, 2, add_cut_distance, "New Cut Distance": unId = unId + 1	hotspot 0, 0, ADD_CUT_DIST - 1, unId, add_cut_distance, 3: unId = unId + 1endiffor i = 1 to n	cutOffset = cut_lengths[i][PAR_CUT_DIST]	cutThickness = cut_lengths[i][PAR_CUT_THICKNESS1]	cutThickness2 = cut_lengths[i][PAR_CUT_THICKNESS2]	cutHeight = cut_lengths[i][PAR_CUT_HEIGHT]	dim1 = A	dim2 = B	if cut_rotations[i] = 90 or cut_rotations[i] = 270 then		dim1 = B		dim2 = A	endif	rotz cut_rotations[i]	! cut distance from bottom	hotspot dim1/2, -dim2/2, 0, unId, cut_lengths[i][PAR_CUT_DIST], 1: unId = unId + 1	hotspot dim1/2, -dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_DIST], 2, cut_lengths[i][PAR_CUT_DIST], "Distance": unId = unId + 1	hotspot dim1/2, -dim2/2, -1, unId, cut_lengths[i][PAR_CUT_DIST], 3: unId = unId + 1	! cut height	hotspot 0, -dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_HEIGHT], 1: unId = unId + 1	hotspot 0, -dim2/2, cutOffset + cut_lengths[i][PAR_CUT_HEIGHT], unId, cut_lengths[i][PAR_CUT_HEIGHT], 2, cut_lengths[i][PAR_CUT_HEIGHT], "Cut Size": unId = unId + 1	hotspot 0, -dim2/2, -1, unId, cut_lengths[i][PAR_CUT_HEIGHT], 3: unId = unId + 1	! cut thickness	hotspot -dim1/2, -dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS1], 1: unId = unId + 1	hotspot -dim1/2, -dim2/2 + cutThickness, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS1], 2, cut_lengths[i][PAR_CUT_THICKNESS1], "Thickness 1": unId = unId + 1	hotspot -dim1/2, -dim2/2 - 1, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS1], 3: unId = unId + 1	if cut_kinds[i] = KIND_LSHAPE then		hotspot dim1/2, dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS2], 1: unId = unId + 1		hotspot dim1/2 - cutThickness2, dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS2], 2, cut_lengths[i][PAR_CUT_THICKNESS2], "Thickness 2": unId = unId + 1		hotspot dim1/2 + 1, dim2/2, cutOffset, unId, cut_lengths[i][PAR_CUT_THICKNESS2], 3: unId = unId + 1	endif	del 1	! cut angle/direction	d = sqr(A*A + B*B) / 2	hotspot 0, -B/2, cutOffset + cutHeight/2, unId, cut_rotations[i], 4+128: unId = unId + 1	hotspot d * sin(cut_rotations[i]), -d * cos(cut_rotations[i]), cutOffset + cutHeight/2, unId, cut_rotations[i], 5, cut_rotations[i], "Rotation": unId = unId + 1	hotspot 0, 0, cutOffset + cutHeight/2, unId, cut_rotations[i], 6: unId = unId + 1	hotspot 0, 0, cutOffset + cutHeight/2 + 1, unId, cut_rotations[i], 7: unId = unId + 1next i