VALUES "n" RANGE[1, 50]!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! When extending, add nodesIF GLOB_MODPAR_NAME = "A" THEN	maxStudX = 0	for i = 1 to n		if X[i] > maxStudX then			maxStudX = X[i]		endif	next i	IF A > maxStudX+defaultStudSpacing AND n < 50 THEN		addCount = INT((A-maxStudX) / defaultStudSpacing)		FOR i = 1 TO addCount			X[n+i] = X[n + i - 1] + defaultStudSpacing		NEXT i		n = n + addCount	ENDIFENDIFPARAMETERS n = nPARAMETERS X = Xparameters lumberVol = lumberVol !lock "LumberSize"parameters frameArea = frameArea