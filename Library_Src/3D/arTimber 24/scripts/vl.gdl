parameters cut_lengths = cut_lengths, n = n, cut_kinds = cut_kinds, cut_rotations = cut_rotationsif GLOB_MODPAR_NAME = "cut_rotations" then	for i = 1 to n 		if cut_rotations[i] > 45 and cut_rotations[i] < 135 then			cut_rotations[i] = 90		endif		if cut_rotations[i] > 135 and cut_rotations[i] < 225 then			cut_rotations[i] = 180		endif		if cut_rotations[i] > 225 and cut_rotations[i] < 315 then			cut_rotations[i] = 270		endif		if cut_rotations[i] > 314 or cut_rotations[i] < 45 then			cut_rotations[i] = 0		endif	next i	parameters cut_rotations = cut_rotationsendifif GLOB_MODPAR_NAME = "cut_lengths" then	for i = 1 to n 		if cut_kinds[i] = KIND_ONESIDE then			if cut_lengths[i][PAR_CUT_THICKNESS1] < 0 then				cut_lengths[i][PAR_CUT_THICKNESS1] = 0			endif			if (cut_rotations[i] = 90 or cut_rotations[i] = 270) and cut_lengths[i][PAR_CUT_THICKNESS1] > A then				cut_lengths[i][PAR_CUT_THICKNESS1] = A			endif			if (cut_rotations[i] = 0 or cut_rotations[i] = 180) and cut_lengths[i][PAR_CUT_THICKNESS1] > B then				cut_lengths[i][PAR_CUT_THICKNESS1] = B			endif		endif	next i	parameters cut_lengths = cut_lengthsendifvalues{2} "orientation" ORIENTATION_VERTICAL, "Vertical", ORIENTATION_HORIZONTAL, "Horizontal"if orientation = ORIENTATION_VERTICAL then	hideparameter "hor_length"	parameters ac_toplevel = ac_bottomlevel + ZZYZXelse	hideparameter "ZZYZX"	parameters ac_toplevel = ac_bottomlevel + Bendif